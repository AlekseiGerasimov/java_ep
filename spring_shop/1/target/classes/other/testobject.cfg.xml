<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name = "objects.Car" table = "car">

        <meta attribute = "class-description">
            Class Car
        </meta>

        <id name = "id" type = "int" column = "id" >
            <generator class="increment" />
        </id>

        <property name = "id" column = "id" type = "int" length="10" not-null="true" insert="false" update="false"/>
        <property name = "model" column = "model" type = "string" length="20" not-null="true" />
        <property name = "color" column = "color" type = "string" length="20" not-null="true" />
        <property name = "modelType" column = "model_type" type = "string" length="20" not-null="true" />
        <property name = "cost" column = "cost" type = "int" length="10" not-null="true" />

    </class>
</hibernate-mapping>


